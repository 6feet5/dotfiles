#include "/home/johan/.themes/terminal/nord"

URxvt.depth: 24
URxvt.font: xft:Source Code Pro for Powerline:size=11:antialias=false
URxvt.cursorColor: nord13
URxvt.scrollBar: false
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true
URxvt.saveLines: 20000
URxvt.keysym.Home: \033[1~
URxvt.keysym.End: \033[4~
URxvt.keysym.KP_Home: \033[1~
URxvt.keysym.KP_End:  \033[4~
URxvt.keysym.Control-Up:    \033[1;5A
URxvt.keysym.Control-Down:  \033[1;5B
URxvt.keysym.Control-Left:  \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C
URxvt.print-pipe: cat > $(mktemp --tmpdir=$XDG_RUNTIME_DIR urxvt-scrollback.XXXXXX)
URxvt.matcher.rend.0: Uline Bold fg6
URxvt.perl-ext-common: default,matcher,resize-font
URxvt.url-launcher: /usr/bin/qutebrowser
URxvt.matcher.button: 1
URxvt.transparent: false
URxvt.pointerBlank: true

Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.autohint:   false
Xft.lcdfilter:  lcddefault

